<view data-event-opts="{{[['tap',[['toDetail',['$event']]]]]}}" class="auth-layout padding-sm  margin-sm margin-tb-sm rounded  bg-white" bindtap="__e"><view class="card-head flex justify-between"><view><text class="card-title">{{item.project_name}}</text></view><view class="card-money">{{''+item.money+'元/天'}}</view></view><view class="card-content"><view class="card-subtitle">{{''+item.addres}}<text class="text-gray margin-lr-md d-inline-block">|</text>{{''+item.subtitle+''}}</view><block wx:for="{{item.tag}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view class="card-tag"><text>{{item}}</text></view></block></block><view class="card-detail"><text>{{item.detail}}</text></view></view></view>